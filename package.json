{
  "name": "guardian",
  "version": "1.0.0",
  "description": "guardian, b/e for ratenride",
  "main": "src/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=test nodemon --exec babel-node src/server.js",
    "build": "babel src --out-dir dist",
    "serve": "node dist/server.js",
    "start": "nodemon --exec babel-node src/server.js",
    "pretest": "cross-env NODE_ENV=test eslint --ignore-path .gitignore . && npm run build",
    "test": "jest --detectOpenHandles --runInBand --forceExit --testTimeout=10000",
    "eslint": "./node_modules/.bin/eslint --ignore-path .gitignore  --ext js --fix . .config"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/testSetup.js"
    ]
  },
  "author": "bkarahan",
  "license": "ISC",
