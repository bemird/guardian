    "morgan": "^1.9.1",
    "node-pre-gyp": "^0.14.0",
    "nodemailer": "^6.4.3",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-local": "^1.0.0",
